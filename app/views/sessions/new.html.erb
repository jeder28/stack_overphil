<div class="ques-answer cf">
  <h3>Login</h3>
  <%= form_tag sessions_path do %>
    <%= label_tag :username %>
    <%= text_field_tag :username %>
    <br><br>
    <%= label_tag :password %>
    <%= password_field_tag :password %>
    <br><br>
    <div class="submit-btn"><%= submit_tag 'Log in' %></div>
  <% end %>
</div>